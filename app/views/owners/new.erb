<h1>Create a new Owner</h1>

<form method="POST" action="/owners">
  <label>Name:</label><br></br>
  <input type="text" name="owner[name]" id="owner_name"><br></br>
  <%@pets.each do |pet|%>
    <input type="checkbox" name="owner[pet_ids][]" id="<%= pet.name %>" value="<%=pet.id%>"><%=pet.name%>
  <%end%><br></br>
  
  <label>name:</label>
  <input  type="text" name="pet[name]" id="pet_name"><br></br>

  <input type="submit" value="Create Owner">
</form>