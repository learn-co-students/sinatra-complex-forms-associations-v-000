<h1>Update Pet</h1>
<form action="/pets/<%=@pet.id%>" method="post">
  <label>Pet Name: </label></br>
  <input type="text" name="pet[name]">
  <br></br>
  <label>Choose an existing owner: </label></br>
  <%@owners.each do |owner| %>
    <input type="checkbox" name="pet[owner_ids][]" value="<%=owner.id%>"
