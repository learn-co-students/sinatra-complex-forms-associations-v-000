<h1>Create a new Pet</h1>
<form class="" action="/pets" method="POST">
  <label>Name:</label>

  <br>

  <input type="text" name="pet_name">

  <br>

  <label>Choose an existing owner:</label>
  <br>

    <%@owners.each do |owner|%>
     <input type="checkbox" name="pet[owner_ids][]" value="<%=owner.id%>" id= "<%=owner.id>"><%=owner.name%></input>
    <%end%>
    
  <br>
</form>
