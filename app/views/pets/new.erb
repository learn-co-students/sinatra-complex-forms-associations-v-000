<h1>Create a new Pet</h1>
<form action="/pets" method="POST">
  <h2>Pet Name:</h2><input type="text" name="pet[name]" id="pet_name">
<br>
  <label>Choose an existing owner:</label>

  <%Owner.all.each do |owner|%>
    <input type="checkbox" name="owner[name][]" id="<%=owner.id%>"

<!-- => #<Owner:0x0000000233aca0 id: 1, name: "Sophie"> -->
     value="<%=owner.name%>"</input>
  <%end%>

  <br></br>

    <label>and/or, create a new owner:</label>
    <br></br>
    <label>name:</label>
      <!-- <input  type="text" name="owner[owner_name]"></input> -->
      <input  type="text" name="owner_name"></input>
    <br></br>
  <input type="submit" id="Create Pet" value="Create Pet">
</form>
